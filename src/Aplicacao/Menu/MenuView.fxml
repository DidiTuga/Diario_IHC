<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Aplicacao.Menu.MenuViewController">
   <children>
      <Label fx:id="lblUser" layoutX="75.0" layoutY="102.0" prefHeight="17.0" prefWidth="119.0" text="Username" />
      <Label fx:id="lblData" alignment="CENTER_RIGHT" layoutX="247.0" layoutY="102.0" prefHeight="17.0" prefWidth="78.0" text="Data" />
      <VBox layoutX="75.0" layoutY="150.0" prefHeight="300.0" prefWidth="250.0" spacing="20.0">
         <children>
            <Button mnemonicParsing="false" onAction="#buttonNovaEntra" prefHeight="25.0" prefWidth="250.0" text="Nova Entrada">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
            <Button fx:id="btLogin" mnemonicParsing="false" onAction="#buttonPesquisar" prefHeight="25.0" prefWidth="250.0" text="Pesquisar">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
            <Button fx:id="btLogin1" mnemonicParsing="false" onAction="#buttonSair" prefHeight="25.0" prefWidth="250.0" text="Sair">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>

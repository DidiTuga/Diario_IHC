            <PasswordField fx:id="txfPassword" prefHeight="25.0" prefWidth="343.0" />
            <ButtonBar prefHeight="40.0" prefWidth="200.0">
               <buttons>
                  <Button fx:id="btLogin" mnemonicParsing="false" onAction="#buttonLogin" text="Entrar" />
